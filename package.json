{
  "name": "beyond-the-build",
  "private": true,
  "scripts": {
    "build": "next build && sanity manifest extract --path public/studio/static",
    "predev": "npm run typegen",
    "clean": "next clean",
    "dev": "next --turbopack",
    "dev2": "next dev",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "next lint .",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "start": "next start",
    "sanity-build": "sanity build",
    "sanity-deploy": "sanity deploy",
    "type-check": "tsc --noEmit",
    "typegen": "sanity schema extract && sanity typegen generate",
    "install-commitizen": "git config --local commitizen.path './node_modules/.bin/cz'",
    "install-husky": "husky && git config core.hooksPath .husky",
    "prepare": "npm run install-husky && npm run install-commitizen"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@next/env": "15.5.4",
    "@sanity/client": "7.11.2",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "1.2.0",
    "@sanity/react-loader": "^1.11.19",
    "@sanity/ui": "^2.15.18",
    "@sanity/vision": "^3.89.0",
    "@tailwindcss/typography": "0.5.18",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "next": "15.5.4",
    "next-sanity": "^9.11.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-live-transitions": "0.2.0",
    "react-icons": "^5.5.0",
    "rxjs": "7.8.2",
    "sanity": "^3.89.0",
    "sanity-plugin-asset-source-unsplash": "3.0.3",
    "server-only": "0.0.1",
    "sonner": "2.0.3",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "beta",
    "commitizen": "^4.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "^9.1.7",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2"
  },
  "config": {
    "commitizen": {
      "path": "@digitalroute/cz-conventional-changelog-for-jira"
    }
  }
}
